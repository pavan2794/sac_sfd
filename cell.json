{
 "id": "com.production.cell",
 "version": "1.0.0",
 "name": "Cell",
 "description": "A production cell",
 "newInstancePrefix": "Cell",
 "icon": "/Icon.png",
 "vendor": "Valantic",
 "eula": "",
 "license": "",
 "webcomponents": [
    {
    "kind": "main",
    "tag": "production-cell",
    "url": "/cell.js",
    "integrity": "",
    "ignoreIntegrity": true
    },
    {
    "kind": "styling",
    "tag": "production-cell-styling",
    "url": "/cell_styling.js",
    "integrity": "",
    "ignoreIntegrity": true
    },
    {
    "kind": "builder",
    "tag": "production-cell-builder",
    "url": "/cell_builder.js",
    "integrity": "",
    "ignoreIntegrity": true
    }
 ],
 "properties": {
    "color": {
    "type": "string",
    "description": "Background color",
    "default": "red"
    },
    "opacity": {
    "type": "number",
    "description": "Opacity",
    "default": 1
    },
    "width": {
    "type": "integer",
    "default": 100
    },
    "height": {
    "type": "integer",
    "default": 100
    }
 },
 "methods": {
    "setColor": {
    "description": "Sets the background color.",
    "parameters": [
    {
    "name": "newColor",
    "type": "string",
    "description": "The new background color"
    }
    ],
    "body": "this.color = newColor;"
    },
    "getColor": {
    "returnType": "string",
    "description": "Returns the background color.",
    "body": "return this.color;"
    }
 },
 "events": {
    "onClick": {
    "description": "Called when the user clicks the Cell."
    }
 }
}
